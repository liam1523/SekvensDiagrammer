<Window x:Class="WPF_sekvensdiagrammer1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_sekvensdiagrammer1"
        xmlns:uc="clr-namespace:WPF_sekvensdiagrammer1.Views.UserControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Background="White">
        <uc:Toolbar/>
        <Frame Content="{Binding PageContent}" Margin="10,10,10,10" Name="PageContentFrame" NavigationUIVisibility="Hidden"></Frame>
    </StackPanel>
</Window>
