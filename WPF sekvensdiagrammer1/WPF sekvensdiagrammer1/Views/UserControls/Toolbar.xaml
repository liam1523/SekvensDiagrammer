<UserControl x:Class="WPF_sekvensdiagrammer1.Views.UserControls.Toolbar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_sekvensdiagrammer1.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="80" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" Background="#BCBBD4">
        <Button Name="Home" Margin="10" Padding="10" Content="Home" Width="75" BorderBrush="Black" BorderThickness="2" FontFamily="Segoe UI"
                Command="{Binding SetPageContent}" CommandParameter="{Binding Name, RelativeSource={RelativeSource Self}}"/>
        <Button Name="LoginPage" Margin="610,10,10,10" Content="Create user" Width="75" BorderBrush="Black" BorderThickness="2" FontFamily="Segoe UI"
                Command="{Binding SetPageContent}" CommandParameter="{Binding Name, RelativeSource={RelativeSource Self}}"/>

    </StackPanel>
</UserControl>
